<h1>Editing: <%= @song.title %> by <%= @song.artist_name %></h1>

<%= render 'form' %>

<%= link_to "Cancel", @song %>
<%= link_to "Delete", @song, method: :delete, data: { confirm: 'Are you sure?' } %>

<%= button_to "Cancel", :back, method: :get, data: { confirm: 'Are you sure you would like to cancel and delete this post?' } %>

<%= button_to "Delete", :back, method: :delete, data: { confirm: 'Are you sure you would like delete this post?' } %>